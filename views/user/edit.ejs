<div class="container" style="margin-top: 100px;">
    <div class="jumbotron">
        <form action="/user/update/<%= user.id %>" method="post" class="form-signin">
            <h2> Estas Editando </h2>

            <input value="<%= user.username %>" name="username" type="text" class="form-control"/>
            <input value="<%= user.userEmail %>" name="userEmail" type="text" class="form-control"/>
            <input value="<%= user.userPassword %>" name="userPassword" type="text" class="form-control"/>
            <input value="Proceed" name="name" type="submit" class="btn btn-lg btn-primary btn-block"/>
            <input value="<%= _csrf %>" name="_csrf" type="hidden" class="form-control"/>
        </form>
    </div>
</div>
